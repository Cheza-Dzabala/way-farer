{
  "name": "wayfarer",
  "version": "1.0.0",
  "description": "Backend service for the way farer application",
  "main": "app.js",
  "dependencies": {
    "@babel/core": "7.5.5",
    "@babel/polyfill": "7.4.4",
    "@babel/register": "7.5.5",
    "@babel/runtime": "7.5.5",
    "@hapi/joi": "15.1.0",
    "babel-preset-airbnb": "4.0.1",
    "babel-runtime": "6.26.0",
    "babel-watch": "7.0.0",
    "bcrypt": "3.0.6",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "cors": "^2.8.5",
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "jsonwebtoken": "8.5.1",
    "make-runnable": "1.3.6",
    "morgan": "1.9.1",
    "pg": "7.12.1",
    "swagger-jsdoc": "3.4.0",
    "swagger-ui-express": "4.0.7"
  },
  "devDependencies": {
    "coveralls": "3.0.5",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-plugin-import": "2.18.2",
    "mocha": "6.2.0",
    "mocha-lcov-reporter": "1.3.0",
    "nyc": "14.1.1"
  },
  "scripts": {
    "start": "NODE_ENV=production npm run create && babel-watch server.js",
    "dev": "NODE_ENV=dev npm run create && babel-watch server.js",
    "test": "NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --require @babel/polyfill --require @babel/register __test__/index.spec.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "deploy": "cd ../../ && git subtree push --prefix server/v2/ heroku/cheza-way-farer master",
    "create": "node --require @babel/polyfill --require @babel/register ./services/dbInit createTables"
  },
  "author": "Macheza Dzabala",
  "license": "ISC"
}
